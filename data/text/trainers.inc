Route126_Text_DeanPostBattle: @ 82A1D72
	.string "I can't find the entrance to\n"
	.string "Sootopolis. Where could it be?$"

Route126_Text_NikkiIntro: @ 82A1DAE
	.string "Ufufufufu!\n"
	.string "I'm a mermaid!$"

Route126_Text_NikkiDefeat: @ 82A1DC8
